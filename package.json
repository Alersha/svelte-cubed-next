{
	"name": "svelte-three",
	"version": "0.0.13",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"package": "svelte-kit package",
		"prepublishOnly": "echo \"Did you mean to run 'svelte-kit package' and publish the generated directory?\" && exit 1",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"postversion": "git push && git push --tags && npm run release",
		"release": "npm run package && npm publish package"
	},
	"devDependencies": {
		"@sveltejs/kit": "next",
		"@types/three": "^0.129.2",
		"eslint": "^7.29.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"prettier": "~2.3.2",
		"prettier-plugin-svelte": "^2.3.1",
		"svelte": "^3.38.3",
		"svelte2dts": "^0.2.12",
		"three": "^0.130.0",
		"three-stdlib": "^2.3.0",
		"typescript": "^4.3.5"
	},
	"type": "module",
	"pnpm": {
		"overrides": {
			"@sveltejs/kit": "link:../../KIT/kit/packages/kit"
		}
	}
}
